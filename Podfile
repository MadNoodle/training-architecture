	pod 'Swifter'
	pod 'Alamofire'
	pod 'Swinject'
	pod 'RxSwift'
	pod 'RxCocoa'
	pod 'RxDataSources'
	pod 'Outlaw', :git => 'https://github.com/Molbie/Outlaw.git'
	pod 'Toaster'
	pod 'SwinjectStoryboard'
end
